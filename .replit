[[workflows.workflow]]
name = "Python Backend Only"
author = 48750695
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd backend && pip install -q uvicorn fastapi sqlalchemy python-multipart pydantic pydantic-settings python-jose passlib bcrypt && python -m uvicorn main:app --host 0.0.0.0 --port 8000 --reload"

[[workflows.workflow]]
name = "Backend Only"
author = 48750695
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd backend && pip install -q -r requirements.txt && python -m uvicorn main:app --host 0.0.0.0 --port 8000 --reload"

[[workflows.workflow]]
name = "Full Stack"
author = 48750695
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd backend && pip install -q -r requirements.txt && python -m uvicorn main:app --host 0.0.0.0 --port 8000 --reload"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[[workflows.workflow]]
name = "Start Full Stack Application"
author = 48751192
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd backend && pip install -q uvicorn fastapi sqlalchemy python-multipart pydantic pydantic-settings python-jose passlib bcrypt psycopg2-binary && python -m uvicorn main:app --host 0.0.0.0 --port 8000 --reload"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[[workflows.workflow]]
name = "Start Full Stack App"
author = 48755284
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd backend && pip install -q -r requirements.txt && python -m uvicorn main:app --host 0.0.0.0 --port 8000 --reload"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "sleep 3 && npm run dev"

[[workflows.workflow]]
name = "Clean Start"
author = 48755284
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd backend && pip uninstall -y pydantic-settings && pip install -q -r requirements.txt && python -m uvicorn main:app --host 0.0.0.0 --port 8000 --reload"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "sleep 5 && npm run dev"

